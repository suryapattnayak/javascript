package exercise.java;

abstract class price implements fruit  {

	price p=new price() 
			{
		
			};
	@Override
	public void taste() {
		// TODO Auto-generated method stub
		System.out.println("taste is sweet");
	}

	@Override
	public void color() {
		// TODO Auto-generated method stub
		System.out.println("color is red");
	}
	public void value() {
		System.out.println("price is 100 per kg");
	}
	public void result()
	{
		int k=p.add(6, 7);
		System.out.println(k);
	}

}
